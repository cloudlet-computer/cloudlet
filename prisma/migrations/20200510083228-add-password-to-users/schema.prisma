datasource db {
  provider = "postgresql"
  url = "***"
}

generator client {
  provider = "prisma-client-js"
}

model User {
  id        Int      @default(autoincrement()) @id
  username  String   @unique
  createdAt DateTime @default(now())
  notes     Note[]
  password  String
}

model Note {
  id        Int      @default(autoincrement()) @id
  createdAt DateTime @default(now())
  title     String
  user      User     @relation(fields: [userId], references: [id])
  userId    Int
}
